#include "stdio.h"
#include "math.h"

int main()
{
    // 2 практика вариант 4
    double a, x, y, z;
    //double y = 4.642 * pow(10, -2);
    //double x = -15.246;
    //double z = 20.001 * 100;
    double xm;
    //x = 4;
    scanf_s("%lf %lf %lf", &x, &y, &z);
    xm = - sqrt(fabs(x));
    a = log(pow(y, xm))*(x-(y/2))+pow(sin(atan(z)), 2);

    //printf("%f %f %f %f", x, y, z, a);
    printf("%lf", a);
    //-15.246 0.04642
}
